; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="FunEmuStation Launcher"
config/description="A simple but elegant launcher of emulators and pc games"
run/main_scene="res://MainScr.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="FunEmuStation Launcher"
run/low_processor_mode=true
boot_splash/image="res://boot_bg.png"
boot_splash/bg_color=Color( 0.0352941, 0.0431373, 0.243137, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

Functions="*res://functions.gd"
Global="*res://global.gd"
Config="*res://config.gd"
SceneChanger="*res://SceneChanger.tscn"
Audio="*res://Audio.tscn"

[display]

window/size/width=1366
window/size/height=768
window/size/borderless=true
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

